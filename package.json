{
  "name": "battery-dashboard",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "./src/generated/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "prisma generate",
    "clean": "rimraf .next out dist coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "seed:quiz": "node scripts/seed-quiz-data.js",
    "sheets:setup": "node scripts/setup-google-sheets.js",
    "sync:sheets": "node scripts/sync-to-google-sheets.js",
    "typescript:sync": "npm run sync:sheets",
    "typescript:backup": "node scripts/backup-system.js",
    "typescript:monitor": "node scripts/data-monitor.js",
    "typescript:quickstart": "./scripts/typescript-healing-quickstart.sh",
    "vercel-build": "prisma generate && prisma db push --accept-data-loss && next build"
  },
